{
    "version": 2,
    "public": true,
    "name": "sjsumsa",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "functions": {
        "api/index.js": {
            "includeFiles": "dist/sjsumsa/**"
        }
    }
}